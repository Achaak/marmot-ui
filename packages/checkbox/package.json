{
  "name": "@pikas-ui/checkbox",
  "version": "2.1.10",
  "private": false,
  "description": "",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas-ui.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas-ui/issues"
  },
  "homepage": "https://github.com/Achaak/pikas-ui#readme",
  "dependencies": {
    "@pikas-ui/icons": "2.1.9",
    "@pikas-ui/styles": "2.3.9",
    "@pikas-ui/text": "3.1.9",
    "@radix-ui/react-checkbox": "^1.0.0",
    "font-color-contrast": "^11.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@pikas-ui/config": "workspace:*",
    "@pikas-ui/tsconfig": "workspace:*",
    "@types/node": "^18.7.11",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "tsup": "^6.2.2",
    "typescript": "4.7.4"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

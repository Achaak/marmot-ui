{
  "name": "@pikas-ui/styles",
  "version": "2.3.5",
  "private": false,
  "description": "",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas-ui.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas-ui/issues"
  },
  "homepage": "https://github.com/Achaak/pikas-ui#readme",
  "dependencies": {
    "@stitches/react": "^1.2.8",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "usehooks-ts": "^2.5.4"
  },
  "devDependencies": {
    "@pikas-ui/config": "workspace:*",
    "@pikas-ui/tsconfig": "workspace:*",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.7.11",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "prettier": "^2.7.1",
    "tsup": "^6.2.2",
    "typescript": "4.7.4"
  }
}

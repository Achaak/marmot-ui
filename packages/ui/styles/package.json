{
  "name": "@pikas-ui/styles",
  "version": "3.5.4",
  "private": false,
  "keywords": [
    "pikas-ui",
    "styles",
    "react"
  ],
  "description": "Styles for Pikas UI",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@stitches/react": "1.2.8",
    "lodash.clonedeep": "4.5.0",
    "lodash.merge": "4.6.2",
    "usehooks-ts": "2.9.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/config": "1.0.0",
    "@pikas/tsconfig": "1.0.0",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/lodash.merge": "4.6.7",
    "@types/node": "18.11.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "cross-env": "7.0.3",
    "eslint": "8.25.0",
    "prettier": "2.7.1",
    "tsup": "6.3.0",
    "typescript": "4.8.4"
  }
}

{
  "name": "@pikas-ui/styles",
  "version": "4.1.0",
  "private": false,
  "keywords": [
    "pikas-ui",
    "styles",
    "react"
  ],
  "description": "A library that provides the styling parameters for the components in the pikas-ui library.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@secrecy/stitches-react": "4.1.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.merge": "4.6.2",
    "usehooks-ts": "3.1.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/prettier": "workspace:*",
    "@pikas/tsconfig": "workspace:*",
    "@types/lodash.clonedeep": "4.5.9",
    "@types/lodash.merge": "4.6.9",
    "@types/node": "20.12.7",
    "@types/react": "18.2.78",
    "@types/react-dom": "18.2.25",
    "cross-env": "7.0.3",
    "eslint": "8.42.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "8.0.2",
    "typescript": "5.4.5"
  }
}

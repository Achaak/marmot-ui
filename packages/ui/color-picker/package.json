{
  "name": "@pikas-ui/color-picker",
  "version": "2.0.1",
  "private": false,
  "keywords": [
    "pikas-ui",
    "color-picker",
    "react"
  ],
  "description": "A UI library that offers a color picker components for React.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@pikas-ui/icons": "4.0.1",
    "@pikas-ui/styles": "4.0.1",
    "@pikas-ui/text": "5.0.1",
    "@pikas-ui/textfield": "4.0.1",
    "@pikas-ui/tooltip": "4.0.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/prettier": "workspace:*",
    "@pikas/tsconfig": "workspace:*",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "cross-env": "7.0.3",
    "eslint": "8.33.0",
    "eslint-config-custom": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "6.5.0",
    "typescript": "4.9.5"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

{
  "name": "@pikas-ui/explorer",
  "version": "1.0.14",
  "private": false,
  "keywords": [
    "pikas-ui",
    "explorer",
    "react"
  ],
  "description": "Components for displaying explorers",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/modifiers": "^6.0.0",
    "@dnd-kit/utilities": "^3.2.0",
    "@pikas-ui/context-menu": "3.3.18",
    "@pikas-ui/dropdown-menu": "3.4.18",
    "@pikas-ui/grid": "1.1.16",
    "@pikas-ui/icons": "3.3.13",
    "@pikas-ui/loader": "3.3.13",
    "@pikas-ui/menu": "3.3.17",
    "@pikas-ui/styles": "3.5.13",
    "@pikas-utils/color": "1.0.4",
    "@pikas-utils/file": "1.0.8",
    "@pikas-utils/keyboard": "1.0.8",
    "@pikas-utils/screen": "1.0.18",
    "usehooks-ts": "2.9.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/prettier": "workspace:*",
    "@pikas/tsconfig": "workspace:*",
    "@types/node": "18.11.10",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "cross-env": "7.0.3",
    "eslint": "8.28.0",
    "eslint-config-custom": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "6.5.0",
    "typescript": "4.9.3"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

{
  "name": "@pikas-ui/table",
  "version": "4.1.7",
  "private": false,
  "keywords": [
    "pikas-ui",
    "table",
    "react"
  ],
  "description": "A UI library that offers a table components for React.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@pikas-ui/button": "4.0.2",
    "@pikas-ui/checkbox": "4.0.2",
    "@pikas-ui/icons": "4.0.2",
    "@pikas-ui/loader": "4.0.2",
    "@pikas-ui/select": "5.1.4",
    "@pikas-ui/styles": "4.0.2",
    "@pikas-ui/textfield": "workspace:^4.0.4",
    "@pikas-utils/color": "2.0.1",
    "@tanstack/match-sorter-utils": "^8.7.6",
    "@tanstack/react-table": "8.7.9",
    "lodash.once": "^4.1.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/prettier": "workspace:*",
    "@pikas/tsconfig": "workspace:*",
    "@types/lodash.once": "^4.1.7",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.34.0",
    "eslint-config-custom": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "6.6.3",
    "typescript": "4.9.5"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

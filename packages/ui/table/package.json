{
  "name": "@pikas-ui/table",
  "version": "3.2.11",
  "private": false,
  "keywords": [
    "pikas-ui",
    "table",
    "react"
  ],
  "description": "Components for creating tables",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@pikas-ui/checkbox": "3.3.9",
    "@pikas-ui/icons": "3.3.6",
    "@pikas-ui/loader": "3.3.6",
    "@pikas-ui/select": "4.3.8",
    "@pikas-ui/styles": "3.5.6",
    "@tanstack/react-table": "8.5.27",
    "font-color-contrast": "11.1.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/prettier": "workspace:*",
    "@pikas/tsconfig": "workspace:*",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "cross-env": "7.0.3",
    "eslint": "8.27.0",
    "eslint-config-custom": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "6.4.0",
    "typescript": "4.8.4"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

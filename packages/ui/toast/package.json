{
  "name": "@pikas-ui/toast",
  "version": "1.2.21",
  "private": false,
  "keywords": [
    "pikas-ui",
    "toast",
    "react"
  ],
  "description": "Components for creating toast elements",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@pikas-ui/button": "3.3.19",
    "@pikas-ui/icons": "3.3.18",
    "@pikas-ui/styles": "3.5.18",
    "@radix-ui/react-toast": "1.1.2"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/prettier": "workspace:*",
    "@pikas/tsconfig": "workspace:*",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "cross-env": "7.0.3",
    "eslint": "8.29.0",
    "eslint-config-custom": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "6.5.0",
    "typescript": "4.9.3"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

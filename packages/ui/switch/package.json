{
  "name": "@pikas-ui/switch",
  "version": "2.1.12",
  "private": false,
  "keywords": [
    "pikas-ui",
    "switch",
    "react"
  ],
  "description": "Components for creating a switch",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@pikas-ui/icons": "3.1.8",
    "@pikas-ui/styles": "3.3.4",
    "@pikas-ui/text": "4.1.6",
    "@pikas-ui/tooltip": "3.1.8",
    "@radix-ui/react-switch": "1.0.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/config": "1.0.0",
    "@pikas/tsconfig": "1.0.0",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "cross-env": "7.0.3",
    "eslint": "8.24.0",
    "tsup": "6.2.3",
    "typescript": "4.8.4"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

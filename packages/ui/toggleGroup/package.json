{
  "name": "@pikas-ui/toggle-group",
  "version": "1.0.3",
  "private": false,
  "keywords": [
    "pikas-ui",
    "toggle-group",
    "react"
  ],
  "description": "A UI library that offers a toggle group components for React.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@pikas-ui/icons": "4.1.0",
    "@pikas-ui/styles": "4.0.5",
    "@pikas-utils/color": "2.0.4",
    "@radix-ui/react-toggle-group": "^1.0.4"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/prettier": "workspace:*",
    "@pikas/tsconfig": "workspace:*",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "cross-env": "7.0.3",
    "eslint": "8.42.0",
    "eslint-config-custom": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "6.7.0",
    "typescript": "5.1.3"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

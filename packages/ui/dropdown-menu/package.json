{
  "name": "@pikas-ui/dropdown-menu",
  "version": "3.2.4",
  "private": false,
  "keywords": [
    "pikas-ui",
    "dropdown-menu",
    "react"
  ],
  "description": "Components for creating dropdown menus",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@pikas-ui/icons": "3.1.4",
    "@pikas-ui/loader": "3.1.4",
    "@pikas-ui/menu": "3.2.3",
    "@pikas-ui/styles": "3.3.1",
    "@radix-ui/react-dropdown-menu": "^1.0.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas/config": "workspace:*",
    "@pikas/tsconfig": "workspace:*",
    "@types/node": "^18.8.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "tsup": "^6.2.3",
    "typescript": "4.8.4"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

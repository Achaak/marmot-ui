{
  "name": "@pikas/docs-routes",
  "version": "1.0.0",
  "private": false,
  "keywords": [
    "pikas-ui",
    "docs-routes",
    "react"
  ],
  "description": "Components for displaying docs-routes",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas/issues"
  },
  "homepage": "https://github.com/Achaak/pikas#readme",
  "dependencies": {
    "@pikas-utils/router": "1.0.6"
  },
  "devDependencies": {
    "@pikas/config": "1.0.0",
    "@pikas/tsconfig": "1.0.0",
    "@types/node": "18.11.2",
    "cross-env": "7.0.3",
    "eslint": "8.25.0",
    "next": "12.3.1",
    "tsup": "6.3.0",
    "typescript": "4.8.4"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

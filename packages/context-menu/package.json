{
  "name": "@pikas-ui/context-menu",
  "version": "3.0.0",
  "private": false,
  "description": "Components for creating context menus",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achaak/pikas-ui.git"
  },
  "author": {
    "name": "Axel Lavoie",
    "email": "axel.lavoie@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Achaak/pikas-ui/issues"
  },
  "homepage": "https://github.com/Achaak/pikas-ui#readme",
  "dependencies": {
    "@pikas-ui/icons": "3.0.0",
    "@pikas-ui/loader": "3.0.0",
    "@pikas-ui/menu": "3.0.0",
    "@pikas-ui/styles": "3.0.0",
    "@radix-ui/react-context-menu": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@pikas-ui/config": "workspace:*",
    "@pikas-ui/tsconfig": "workspace:*",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "tsup": "^6.2.3",
    "typescript": "4.8.2"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
}

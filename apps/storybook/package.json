{
  "name": "@pikas-ui/storybook",
  "version": "1.0.8",
  "private": true,
  "scripts": {
    "build": "build-storybook",
    "dev": "start-storybook -p 6006",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "eslint \"./**/*.{ts,tsx,json}\" --fix",
    "start": "npx http-server ./storybook-static -p 3000",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf storybook-static"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@pikas-ui/button": "1.0.8",
    "@pikas-ui/icons": "1.0.8",
    "@pikas-ui/loader": "1.0.8",
    "@pikas-ui/styles": "1.0.8",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@pikas-ui/config": "workspace:*",
    "@pikas-ui/tsconfig": "workspace:*",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-storyshots": "^6.4.21",
    "@storybook/addon-storysource": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/cli": "^6.4.21",
    "@storybook/components": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@storybook/theming": "^6.4.21",
    "babel-loader": "^8.2.5"
  }
}

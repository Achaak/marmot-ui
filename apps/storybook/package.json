{
  "name": "@pikas-ui/storybook",
  "version": "1.0.62",
  "private": true,
  "scripts": {
    "build": "build-storybook",
    "dev": "start-storybook -p 6006",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "eslint \"./**/*.{ts,tsx,json}\" --fix",
    "start": "npx http-server ./storybook-static -p 3000",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf storybook-static"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@pikas-ui/alert": "2.0.9",
    "@pikas-ui/button": "2.0.10",
    "@pikas-ui/checkbox": "2.0.11",
    "@pikas-ui/context-menu": "2.0.9",
    "@pikas-ui/dropdown-menu": "2.0.9",
    "@pikas-ui/icons": "2.0.9",
    "@pikas-ui/loader": "2.0.8",
    "@pikas-ui/searchbar": "2.0.13",
    "@pikas-ui/select": "3.0.3",
    "@pikas-ui/separator": "2.0.8",
    "@pikas-ui/slider": "2.0.11",
    "@pikas-ui/styles": "2.1.7",
    "@pikas-ui/table": "2.0.14",
    "@pikas-ui/text": "3.0.2",
    "@pikas-ui/textarea": "2.0.12",
    "@pikas-ui/textfield": "2.0.12",
    "@pikas-ui/tooltip": "2.0.8",
    "http-server": "^14.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@pikas-ui/config": "workspace:*",
    "@pikas-ui/tsconfig": "workspace:*",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-storyshots": "^6.5.6",
    "@storybook/addon-storysource": "^6.5.6",
    "@storybook/addons": "6.5.9",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/cli": "^6.5.6",
    "@storybook/components": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/theming": "^6.5.6",
    "babel-loader": "^8.2.5",
    "eslint": "8.18.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
